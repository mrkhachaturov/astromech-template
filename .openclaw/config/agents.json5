    // â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    // Heartbeat & Concurrency
    // â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    // Periodic agent turns for background monitoring and check-ins
    // ğŸ“„ Docs: /path/to/astromech/tools/wiki/source/docs/gateway/heartbeat.md
    // ğŸ¤– Wiki: just wiki-docs "heartbeat configuration activeHours"
    "heartbeat": {
      "every": "4h",                    // Check every 4 hours
      "model": "openai-codex/gpt-5.1-codex-mini",  // Mini model for heartbeats â€” faster, cheaper, 400K context
      "includeReasoning": false,        // Don't send separate Reasoning: messages
      "target": "last",                 // Send to last used channel
      "ackMaxChars": 300,               // Max chars after HEARTBEAT_OK before delivery

