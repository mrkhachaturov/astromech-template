    // â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    // Memory Search (Vector + Hybrid Search + Session Memory)
    // â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    // Semantic search over workspace memory files AND session history
    // Uses OpenAI embeddings with Gemini fallback + sqlite-vec acceleration
    // Combines vector embeddings with keyword search via hybrid RRF fusion
    // Session memory is experimental and indexes conversation history
    // ğŸ“„ Docs: /path/to/astromech/tools/wiki/source/docs/concepts/memory.md:79-142
    // ğŸ“„ Docs: /path/to/astromech/tools/wiki/source/docs/concepts/memory.md:473-542
    // ğŸ¤– Wiki: just wiki-docs "memory session search experimental"
    // ğŸ¤– Wiki: just wiki-docs "sqlite-vec vector acceleration"
    "memorySearch": {
      "provider": "openai",
      "model": "text-embedding-3-small",
      "fallback": "gemini",

      // Experimental: Enable session memory indexing
      // Indexes conversation history for semantic recall across sessions
      "experimental": {
        "sessionMemory": true
      },
      "sources": ["memory", "sessions"],

      // OpenAI Batch API for faster bulk indexing
      "remote": {
        "batch": {
          "enabled": true,
          "wait": true,           // Wait for batch completion (ensures complete indexing)
          "concurrency": 2,       // 2 parallel batch jobs
          "pollIntervalMs": 2000, // Check batch status every 2s
          "timeoutMinutes": 60    // 1-hour timeout for large indexing jobs
        }
      },

      // SQLite-vec: Accelerates vector queries inside SQLite
      // Falls back to JS cosine similarity if extension unavailable
      "store": {
        "vector": {
          "enabled": true
        }
      },

      // Sync configuration: when to trigger indexing
      "sync": {
        "watch": true,  // Auto-reindex when memory files change

        // Session indexing thresholds (debounced, async)
        "sessions": {
          "deltaBytes": 100000,   // Index after ~100 KB of new session data
          "deltaMessages": 50     // OR after 50 new messages (whichever first)
        }
      },

      // Hybrid search: combines vector similarity + BM25 keyword matching
      "query": {
        "hybrid": {
          "enabled": true,
          "vectorWeight": 0.7,          // 70% weight to semantic similarity
          "textWeight": 0.3,            // 30% weight to keyword matching
          "candidateMultiplier": 4,     // Fetch 4x candidates before fusion

          // Diversity: reduces duplicate/near-identical results
          // When daily notes repeat the same topic across multiple days,
          // agent gets varied snippets instead of 5 nearly identical ones
          "mmr": {
            "enabled": true,
            "lambda": 0.7               // 0 = max diversity, 1 = pure relevance
          },

          // Recency: boosts newer memories, decays older ones exponentially
          // Prevents old well-worded notes from outranking yesterday's update
          // halfLife 30d: 7 days ago = 84%, 30 days = 50%, 3 months = 12.5%
          // MEMORY.md and non-dated files (memory/projects.md) are never decayed
          "temporalDecay": {
            "enabled": true,
            "halfLifeDays": 30
          }
        }
      },

      // Embedding cache: speeds up repeated queries
      "cache": {
        "enabled": true,
        "maxEntries": 50000
      }
    },

    // â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    // Context Management
    // â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    // Context pruning: removes old messages based on TTL or cache strategy
    // Compaction: triggers memory flush before context limit
    // ğŸ“„ Docs: /path/to/astromech/tools/wiki/source/docs/concepts/compaction.md:44-62
    // ğŸ¤– Wiki: just wiki-docs "context pruning compaction"
    "contextPruning": {
      "mode": "cache-ttl",
      "ttl": "1h",
      "keepLastAssistants": 3
    },

    "compaction": {
      "mode": "safeguard",
      "reserveTokensFloor": 20000,
      "memoryFlush": {
        "enabled": true,
        "softThresholdTokens": 6000,
        "systemPrompt": "Session nearing compaction. Store durable memories now.",
        "prompt": "Write any lasting notes to memory/YYYY-MM-DD.md; reply with NO_REPLY if nothing to store."
      }
    },

    // â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    // Streaming & Response
    // â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
